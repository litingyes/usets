{
  "name": "usexx",
  "type": "module",
  "version": "0.0.0",
  "private": false,
  "description": "a tool kit for web development",
  "author": "litingyes <luz.liting@gmail.com> (https://litingyes.top/)",
  "license": "MIT",
  "homepage": "https://github.com/litingyes/usets#readme",
  "bugs": {
    "url": "https://github.com/litingyes/usets/issues"
  },
  "keywords": [
    "toolkit",
    "typescript",
    "javascript",
    "ts",
    "js"
  ],
  "exports": {
    ".": {
      "types": {
        "import": "./dist/esm/index.d.ts",
        "require": "./dist/cjs/index.d.ts"
      },
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./min": {
      "types": {
        "import": "./dist/min/index.d.mts",
        "require": "./dist/min/index.d.ts"
      },
      "import": "./dist/min/index.mjs",
      "require": "./dist/min/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "commit": "git-cz",
    "prebuild": "jiti scripts/export.ts",
    "build": "unbuild",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "preprelease": "pnpm build && pnpm test",
    "release": "bumpp",
    "prepare": "husky"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.2",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@tsconfig/node-lts": "^20.1.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "22",
    "@vitest/coverage-istanbul": "^2.1.8",
    "bumpp": "^9.5.2",
    "commitizen": "^4.3.1",
    "eslint": "^9.10.0",
    "eslint-plugin-format": "^0.1.3",
    "happy-dom": "^16.2.9",
    "husky": "^9.1.7",
    "inquirer": "^10.2.2",
    "lint-staged": "^15.2.11",
    "typescript": "^5.6.3",
    "unbuild": "^3.1.0",
    "vitest": "^2.1.8"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
